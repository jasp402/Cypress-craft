  "type": "commonjs",
  "name": "pack-manager",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "pm:frontend": "vite --config frontend/vite.config.ts --host --port 5173",
    "pm:backend": "node backend/src/server.js",
    "pm:start": "node -e \"require('child_process').spawn('npm', ['run','pm:backend'], {stdio:'inherit', shell:true}); require('child_process').spawn('npm', ['run','pm:frontend'], {stdio:'inherit', shell:true});\"",
    "pm:build": "vite build --config frontend/vite.config.ts",
    "pm:start:prod": "npm run pm:build && node backend/src/server.js",

    "dev": "vite",
    "build": "tsc -p frontend/tsconfig.json && vite build --config frontend/vite.config.ts",
    "preview": "vite preview --config frontend/vite.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "server": "ts-node src/backend/server.ts",
    "server:watch": "nodemon --watch src/backend src/backend/server.ts",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
